.faw-carousel-outer{
    text-align: center;
    overflow: hidden;
    background-image: none;
    background-repeat: no-repeat;
    background-size: cover;
    height: auto;
    position: relative;
    padding-bottom: 0em;
    
    @include breakpoint(md) {
         padding-bottom: 6em;
    }
}

.faw-carousel{
    width: 100%;
    display: inline-block;
    overflow: visible!important;
    background-color: #fff;
    padding-bottom: 0;
    position: relative;
    z-index: 1;
    overflow: hidden!important;
}



.faw-carousel-wrapper{
    overflow: visible;
    display: inline-block;
    width: 100%;
    overflow: visible!important;
    
    @include breakpoint(md) {
         width: 560px;
    }
    
    .owl-wrapper-outer{
        overflow: visible!important;
    }
}

.faw-carousel-item{
    background-color: #FFF;
    width: 100%;
    height: 100%;
    margin-left: 0;
    border-radius: 0;
    border-radius: 0;
    text-align: center;
    padding: 1em;
    padding-top: 1.5em;
    margin-left: 0;
    margin-top: 0;
    cursor: pointer;

    @include breakpoint(md) {
       width: 500px;
       height: 500px;
       margin-left: 30px;
       border-radius: 50%;
       margin-top: 10em;
       padding: 3em;
       padding-top: 4.5em;
    }
}

.faw-carousel-copy{
    margin-top: 2em;
    margin-bottom: 2em;
}

.faw-carousel-bullets{
    background-color: #FFF;
    @include breakpoint(md) {
       background-color: transparent;
    }
    
    .carousel-bullets-item-active{
        background: #000;
        @include breakpoint(md) {
           background: #FFF;
        }
    }
}

.faw-carousel-bullets li{
    margin: 0.3em;
    margin-top: 1em;
    margin-bottom: 1em;
    cursor: pointer;
    border: 1px #000 solid;
    
    @include breakpoint(md) {
       margin-top: 5em;
       border: 1px #FFF solid;
    }
}

.faw-carousel .responsive-image img{
  max-width: none;
}
